    public class Employee
    {
        public static int EmpNum;
        public Employee(string FName, string LName, int EmpNum)
        {
            FName = "Mario";
            LName = "Ceballos";
            EmpNum++;
        }
       
        
    }
    public class Vendor
    {
        public static int VendNum;
        static Vendor() => VendNum = 0;
        public Vendor (int VendNum, string VendName, string VendInd)
        {
            
            VendName = "Delta";
            VendInd = "Airline";
            VendNum++;
        }
    }
